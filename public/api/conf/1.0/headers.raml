#%RAML 1.0 Library

usage: Collection of headers required for the request

traits:
  contentTypeJson:
    headers:
      Content-Type:
        description: Specifies the format of the request body, which must be JSON.
        type: string
        required: true
        example: application/json

  acceptJson:
    headers:
      Accept:
        description: Specifies the response format and the [version](/api-documentation/docs/reference-guide#versioning) of the API to be used.
        type: string
        required: true
        example: application/vnd.hmrc.1.0+json

  correlationID:
    headers:
      CorrelationId:
        description: A UUID identifier of the transaction.<p>Must conform to the regular expression</p><p>`^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$`</p>
        type: string
        required: true
        example: a1e8057e-fbbc-47a8-a8b4-78d9f015c253

  originatorID:
    headers:
      OriginatorId:
        description: Mandatory Consumer ID.
        type: string
        required: true
        example: DA2_DWP_REG
